    "next-sitemap": "^4.2.3",
    "postcss-import": "^15.1.0",
    "tailwind-nord": "^1.3.0",
    "tailwind-outline-plugin": "^2.0.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
